{
	"common": {
		"operator": { "operation_type": "IN" },
		"super_efecto": {
			"effect_type": "no_effect",
			"effect_length": 50,
			"delay_image": 0,
			"apply_to_sound": false,
			"reverse_out_effect": false,
			"mirror_horizontal_out_effect": false,
			"mirror_vertical_out_effect": false
		},
		"expected_result": "FINISHED",
		"expected_calls": {
			"new_effect": null
		},
		"expected_report": null
	},
	
	"tests": [
		{	"name": "test create IN effect: delay frame_offset_start",
			"super_efecto": { "delay_image": 10 },
			"sequences": [
				{
					"name": "MySequence",
					"type": "MOVIE",
					"channel": 1,
					"select": true,
					"frame_final_duration": 100,
					"frame_offset_start": 25
				}
			],
			"expected_sequences": [
				{
					"name": "MySequence",
					"type": "MOVIE",
					"channel": 1,
					"select": true,
					"frame_final_duration": 100,
					"frame_offset_start": 35
				}
			]
		},
		{	"name": "test create IN effect: initial values",
			"super_efecto": {
				"delay_image": 10,
				"initial_position_x": 21,
				"initial_position_y": 31,
				"initial_zoom": 0.41,
				"initial_opacity": 0.51,
				"initial_offset_x": 61,
				"initial_offset_y": 71
			},
			"sequences": [
				{
					"name": "MySequence",
					"type": "TRANSFORM",
					"blend_type": "FOO",
					"channel": 1,
					"select": true,
					"frame_final_duration": 100,
					"frame_offset_start": 25,
					"transform" : {
						"offset_x": 11,
						"offset_y": 21
					}
				}
			],
			"expected_sequences": [
				{
					"name": "MySequence",
					"type": "TRANSFORM",
					"blend_type": "ALPHA_OVER",
					"channel": 1,
					"select": true,
					"frame_final_duration": 100,
					"frame_offset_start": 35,
					"translate_start_x": 21,
					"translate_start_y": 31,
					"use_uniform_scale": true,
					"scale_start_x": 0.41,
					"blend_alpha": 0.51,
					"use_translation": true,
					"transform" : {
						"offset_x": 1182.2,
						"offset_y":  787.8
					}
				}
			]
		},
		{	"name": "test create IN effect: tranform animation",
			"super_efecto": {
				"delay_image": 9,
				"initial_position_x": 21,
				"position_x_animated": true,
				"final_position_x": 121,
				"initial_position_y": 51,
				"position_y_animated": true,
				"final_position_y": 191,
				"initial_zoom": 0.1,
				"zoom_animated": true,
				"final_zoom": 1.2,
				"initial_opacity": 0.2,
				"opacity_animated": true,
				"final_opacity": 0.9,				
				"initial_offset_x": 53,
				"offset_x_animated": true,
				"final_offset_x": 153,
				"initial_offset_y": 72,
				"offset_y_animated": true,
				"final_offset_y": 165
			},
			"sequences": [
				{
					"name": "MySequence",
					"type": "TRANSFORM",
					"blend_type": "FOO",
					"channel": 1,
					"select": true,
					"frame_final_duration": 100,
					"frame_final_start": 33,
					"frame_final_end": 133,
					"scale_start_x": 1.2,
					"transform" : {
						"offset_x": 11,
						"offset_y": 21
					}
				}
			],
			"expected_sequences": [
				{
					"name": "MySequence",
					"type": "TRANSFORM",
					"blend_type": "ALPHA_OVER",
					"channel": 1,
					"select": true,
					"frame_final_duration": 100,
					"translate_start_x": 121,
					"translate_start_y": 191,
					"scale_start_x": 1.2,
					"blend_alpha": 0.9,
					"transform" : {
						"offset_x": 2948.6,
						"offset_y": 1803.0
					}
				}
			],
			"expected_calls": {
				"sequences_keyframe_insert": [
					[	
						[ "translate_start_x", { "index": -1, "frame": 42 } ],
						[ "translate_start_x", { "index": -1, "frame": 92 } ],
						[ "translate_start_y", { "index": -1, "frame": 42 } ],
						[ "translate_start_y", { "index": -1, "frame": 92 } ],
						[ "scale_start_x", { "index": -1, "frame": 42 } ],
						[ "scale_start_x", { "index": -1, "frame": 92 } ],
						[ "blend_alpha", { "index": -1, "frame": 42 } ],
						[ "blend_alpha", { "index": -1, "frame": 92 } ]
					]
				],
				"sequences_transform_keyframe_insert": [
					[
						[ "offset_x", { "index": -1, "frame": 42 } ],
						[ "offset_x", { "index": -1, "frame": 92 } ],
						[ "offset_y", { "index": -1, "frame": 42 } ],
						[ "offset_y", { "index": -1, "frame": 92 } ]
					]
				]
			},
			"sequences_keyframe_insert_values": [
				{ "sequence": "MySequence", "name": "translate_start_x", "frame": 42, "value":  21 },
				{ "sequence": "MySequence", "name": "translate_start_x", "frame": 92, "value": 121 },
				{ "sequence": "MySequence", "name": "translate_start_y", "frame": 42, "value":  51 },
				{ "sequence": "MySequence", "name": "translate_start_y", "frame": 92, "value": 191 },
				{ "sequence": "MySequence", "name": "scale_start_x", "frame": 42, "value": 0.1 },
				{ "sequence": "MySequence", "name": "scale_start_x", "frame": 92, "value": 1.2 },
				{ "sequence": "MySequence", "name": "blend_alpha", "frame": 42, "value": 0.2 },
				{ "sequence": "MySequence", "name": "blend_alpha", "frame": 92, "value": 0.9 },
				{ "sequence": "MySequence.transform", "name": "offset_x", "frame": 42, "value": 1028.6 },
				{ "sequence": "MySequence.transform", "name": "offset_x", "frame": 92, "value": 2948.6 },
				{ "sequence": "MySequence.transform", "name": "offset_y", "frame": 42, "value":  798.6 },
				{ "sequence": "MySequence.transform", "name": "offset_y", "frame": 92, "value": 1803.0 }
			]
		},
